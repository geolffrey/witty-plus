<?xml version="1.0" encoding="utf-8"?>
<messages>
  <!-- User Edit Interface -->
  <message id="user-edit-template">
      <div class="field-row">
        <span class="label">${name-label}</span>
        <span class="widget">${name-field}</span>
        <span class="help-text">&nbsp;${name-msg}</span>
      </div>
      <div class="field-row">
        <span class="label">${new-password-label}</span>
        <span class="widget">${new-password-field}</span>
        <span class="help-text">&nbsp;${new-password-msg}</span>
      </div>
      <div class="field-row">
        <span class="label">${new-password2-label}</span>
        <span class="widget">${new-password2-field}</span>
        <span class="help-text">&nbsp;${new-password2-msg}</span>
      </div>
      ${btn-bar}
  </message>
  <message id="userList-label">Users:</message>
  <message id="name-label">Name:</message>
  <message id="new-password-label">New Password:</message>
  <message id="new-password2-label">Repeat Password:</message>
  <message id="save-changes?">Save changes?</message>
  <message id="save-new-password-user-x">Save the new password for user {1} ?</message>
  <message id="change-username-from-x-to-y">About to change Username from '{1}' to '{2}'. Are you sure ?</message>
  <message id="cant-save">Can't save changes</message>
  <message id="you-didnt-change-anything">You didn't change anything</message>
  <message id="user-name-x-exists">The username '{1}' already exists</message>
  <message id="user-needs-a-name">User needs a name</message>
  <message id="new-user-needs-a-password">New User needs a password</message>
  <message id="passwords-dont-match">Passwords provided don't match</message>
  <message id="Adding a new user">Adding a new user</message>
  <message id="fix-up-fields">Please fix the highlighted fields</message>
</messages>

