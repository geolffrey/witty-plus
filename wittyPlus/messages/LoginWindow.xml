<?xml version="1.0" encoding="utf-8"?>
<messages>
  <!-- Login Window Messages -->
  <message id="login-template">
      <div class="field-row">
          <span class="label">${username-label}</span>
          <span class="widget">${username-field}</span>
          <span class="help-text">&nbsp;</span>
      </div>
      <div class="field-row">
          <span class="label">${password-label}</span>
          <span class="widget">${password-field}</span>
          <span class="help-text">&nbsp;</span>
      </div>
      ${btn-bar}
  </message>
  <message id="username-label">Username:</message>
  <message id="password-label">Password:</message>
  <message id="Login">Login</message>
  <message id="Logout">Logout</message>
  <message id="invalid-login">Wrong username or password</message>
  <message id="Login Cancelled">Login Cancelled</message>
  <message id="OK">OK</message>
  <message id="Cancel">Cancel</message>
</messages>

